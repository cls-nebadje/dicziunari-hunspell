<?xml version="1.0"?>

<!-- Template variables (pre- and postfixed with double underscore (__):

     ID      : rm-Vallader@dictionaries.addons.mozilla.org
     VERSION : add on version, e.g. 2.0.3
     NAME_EN : human readable dictionary name english, e.g. Rhaeto-Romance Dictionary (Vallader)
     DESC_EN : description english
     AUTH_EN : author english
     NAME_DE : human readable dictionary name german, e.g. Rhätoromanisches Wörterbuch (Vallader)
     DESC_DE : decription german 
     AUTH_DE : author german
-->

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>__ID__</em:id>
    <em:type>64</em:type>
    <em:version>__VERSION__</em:version>
    <em:name>__NAME_EN__</em:name>
    <em:description>__DESC_EN__</em:description>
    <em:creator>__AUTH_EN__</em:creator>
    <em:unpack>true</em:unpack>
    <em:localized>
      <Description>
        <em:locale>de</em:locale>
        <em:name>__NAME_DE__</em:name>
        <em:description>__DESC_DE__</em:description>
        <em:creator>__AUTH_DE__</em:creator>
      </Description>
    </em:localized>
    <!-- Firefox -->
